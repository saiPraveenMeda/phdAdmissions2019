{% load staticfiles %}
<!DOCTYPE html>
<html>
<head>
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<title>Print Annexures</title>

	<!-- jQuery library -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

	<!-- Latest compiled JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

	<style type="text/css">
		hr{
			border: none;
			height: 1px;
			background-color: black;
		}
		@media print {
    		@page { size: landscape; };
			}
	</style>

</head>

<body>
		<div class="row" style="background-color:#003A6A; color: white;">
				<center><h2>Annexures</h2></center>
			</div>
<div class="row">
	<div class="col-sm-10 col-sm-offset-1">
	<form id="annexure" style="margin-top:3%;" action="" method="POST">
		{% csrf_token %}
<div class="row">
			<center><h4>Annexure - A (Qualifications) </h4></center><br>
</div>
<table class="table table-bordered">
	<thead>
        <tr style="background-color:#e3e3e3;">
	        <th>S.No.</th>
	        <th>Qualification</th>
	        <th>Degree</th>
	        <th>%Mark/CGPA</th>
	        <th>Div.</th>
	        <th colspan="2"><center>Date</center></th>
	        <th>University/Institution</th>
        </tr>
        <tr style="background-color:#e3e3e3;">
        	<th colspan="5"></th>
            <th>Entry</th>
            <th>Completion</th>
            <th></th>
        </tr>
    </thead>
    <tbody id="annexure_a">
    	{% for rec in annex_a %}
    	<tr>
    		<td>{{forloop.counter}}</td>
    		<td>{{rec.qualification_a}}</td>
    		<td>{{rec.degree_a}}</td>
    		<td>{{rec.marks_a}}</td>
    		<td>{{rec.div_a}}</td>
    		<td>{{rec.entry_a}}</td>
    		<td>{{rec.completion_a}}</td>
    		<td>{{rec.university_a}}</td>
    	</tr>
    	{% endfor %}
    </tbody>
</table>
<div class="pagebreak"> </div>
		<div class="row">
			<center><h4>External Sponsored R&D Projects completed or ongoing/patent granted (Annexure E1)</h4></center><br>
		</div>
		<div id="part2">
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th>S.No.</th>
			<th>Title of R&D Project</th>
			<th>Completed/Ongoing</th>
			<th colspan="2"><center>Duration</center></th>
			<th>Funding Agency</th>
			<th>Amount in  Lakhs</th>
			<th colspan="2"><center>Role</center>
			<th>Credit Points</th>
		</tr>
		<tr style="background-color:#e3e3e3;">
			<th colspan="3"></th>
			<th>From</th>
			<th>To</th>
			<th colspan="2"></th>
			<th>As PI or Co PI</th>
			<th>Total No. of Co PIs for the project</th>
			<th></th>
		</tr>
	</thead>
	<tbody id="annexure_e1">
		{% for item in annex_e1 %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.progress}}</td>
			<td>{{item.funding_agency}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to}}</td>
			<td>{{item.amount}}</td>
			<td>{{item.pi_co}}</td>
			<td>{{item.total_co_pi}}</td>
			<td>{{item.credit}}</td>
		</tr>
		{% endfor %}

	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Patent Details (Annexure E2)</h4></center><br>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Patent Title</th>
			<th>Status</th>
			<th>Details</th>
			<th colspan="2">Role</th>
			<th colspan="2">Credit Points</th>
		</tr>
		<tr style="background-color:#e3e3e3;">
			<th colspan="2"></th>
			<th>Applied/Granted</th>
			<th>No. & Date of application/grant</th>
			<th>Principal Inventor or Coinventor</th>
			<th>Total No of Co Inventors in patent</th>
			<th colspan="2"></th>
		</tr>
	</thead>
	<tbody id="annexure_e2">
		{% for item in annex_e2 %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.patent}}</td>
			<td>{{item.status}}</td>
			<td>{{item.details}}</td>
			<td>{{item.principal_role}}</td>
			<td>{{item.total_coinv}}</td>
			<td>{{item.credit}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Consultancy Project (Annexure F)</h4></center><br>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Sponsoring Agency</th>
			<th>Completion Date</th>
			<th>Number of Coordinators</th>
			<th>Consultancy Amount</th>
		</tr>
	</thead>
	<tbody id="annexure_f">
		{% for item in annex_f %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.spons_ag}}</td>
			<td>{{item.comp_date}}</td>
			<td>{{item.noc}}</td>
			<td>{{item.con_amt}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>PhD Completed(Including thesis submitted cases) (Annexure G)</h4></center><br>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Name of Research Scholar</th>
			<th>Title of Thesis</th>
			<th>Status(Submitted/Awarded)</th>
			<th>Date of Submission/PhD Award</th>
			<th colspan="2">Role</th>
			<th>Credit Points</th>
		</tr>
		<tr style="background-color:#e3e3e3;">
			<th colspan="5"></th>
			<th>As Main/Co Supervisor</th>
			<th>Total No of Co Supervisor/s</th>
			<th></th>
		</tr>
	</thead>
	<tbody id="annexure_g">
		{% for item in annex_g %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.res_name}}</td>
				<td>{{item.thesis}}</td>
				<td>{{item.status}}</td>
				<td>{{item.sub_date}}</td>
				<td>{{item.role_main}}</td>
				<td>{{item.co_sup}}</td>
				<td>{{item.credit}}</td>
			</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Journal Papers in SCSI/Scopus(Paid Journal Papers not allowed) (Annexure H)</h4></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_h_last_prom" value="{{annex_h_last_prom}}" required>
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Title of the Paper</th>
			<th>Name of Journal Volume,pg year</th>
			<th>Is it paid Journal(Y/N)?</th>
			<th>Indexed in Scopus/SCI (Y/N)</th>
			<th colspan="2">Role</th>
			<th>Credit Points</th>
		</tr>
	</thead>
	<tbody id="annexure_h">
		<tr style="background-color:#e3e3e3;">
			<td colspan="5"></td>
			<td>As First Author/Main Supervisor/Other Authors</td>
			<td>Total No of Other Authors</td>
			<td></td>
		</tr>
		{% for item in annex_h %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.journal}}</td>
			<td>{{item.paid}}</td>
			<td>{{item.indexed}}</td>
			<td>{{item.roles}}</td>
			<td>{{item.no_authors}}</td>
			<td>{{item.credit}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Conference Paper Indexed (Annexure I)</h4></center><br>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Title of the Paper</th>
			<th>Proceeding Details</th>
			<th>Indexed in Scopus/SCI (Y/N)</th>
			<th colspan="2">Role</th>
			<th>Credit Points</th>
		</tr>
	</thead>
	<tbody id="annexure_i">
		<tr style="background-color:#e3e3e3;">
			<td colspan="2"></td>
			<td>Name of Conference Pg,Year</td>
			<td></td>
			<td>As First Author/Main Supervisor/Other Authors</td>
			<td>Total No of Other Authors</td>
			<td colspan="2"></td>
		</tr>
		{% for item in annex_i %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.conference}}</td>
			<td>{{item.indexed}}</td>
			<td>{{item.roles}}</td>
			<td>{{item.no_authors}}</td>
			<td>{{item.credit}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Administrative assignments-I (Annexure J)</h4><br><p>(HoD/Dean/Chief Warden/<u>Prof.I/C (T&P)</u>/Advisor(Estate)/CVO/PI(Exam)/TEQIP(coordinator)</p></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_j_last_prom" value="{{annex_j_last_prom}}" >
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Assignment</th>
			<th colspan="2">Date</th>
			<th>No. of Semester</th>
		</tr>
	</thead>
	<tbody id="annexure_j">
		<tr style="background-color:#e3e3e3;">
			<td colspan="2"></td>
			<td>From</td>
			<td>To</td>
			<td colspan="1"></td>
		</tr>
		{% for item in annex_j %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.sem}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Administrative assignments-II (Annexure K)</h4><br>
	<p>Faulty in-charge(Comp centre/IT Serviices/Library/Admission/<u>student activities</u>/<u>other institutional activities(please specify) )</u></p></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_k_last_prom" value="{{annex_k_last_prom}}" >
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Assignment</th>
			<th colspan="2">Date</th>
			<th>No. of Semester</th>
		</tr>
	</thead>
	<tbody id="annexure_k">
		<tr style="background-color:#e3e3e3;">
			<td colspan="2"></td>
			<td>From</td>
			<td>To</td>
			<td colspan="1"></td>
		</tr>
		{% for item in annex_k %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.sem}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Administrative assignments-III (Annexure L)</h4>
	<br>
	<p>(Chairman and Convener of different standing committee and special committee(Ex officio status will not be considered)<br>Faulty in-charge(<u>each for one year duration</u>) of different units or equivalent not covered above</p></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_l_last_prom" value="{{annex_l_last_prom}}" >
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Assignment</th>
			<th colspan="2">Date</th>
			<th>Is the assignment more than 1 year</th>
			<th>No. of Semester</th>
		</tr>
	</thead>
	<tbody id="annexure_l">
		<tr style="background-color:#e3e3e3;">
			<td colspan="2"></td>
			<td>From</td>
			<td>To</td>
			<td colspan="2"></td>
		</tr>
		{% for item in annex_l %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.assign}}</td>
			<td>{{item.sem}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Administrative assignments-IV (Annexure M)</h4>
	<br>
	<p>(Departmental activities identified by HoD like lab in charges or departmental committee for minimum period of one year)</p></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_m_last_prom" value="{{annex_m_last_prom}}" >
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Assignment</th>
			<th colspan="2">Date</th>
			<th>Is the assignment more than 1 year</th>
			<th>No. of Semester</th>
		</tr>
	</thead>
	<tbody id="annexure_m">
		<tr style="background-color:#e3e3e3;">
			<td colspan="2"></td>
			<td>From</td>
			<td>To</td>
			<td colspan="2"></td>
		</tr>
		{% for item in annex_m %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.assign}}</td>
			<td>{{item.sem}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Workshop/FDP/STTP (Annexure N)</h4>
	<br>
	<p>(Departmental activities identified by HoD like lab in charges or departmental committee for minimum period of one year)</p></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_n_last_prom" value="{{annex_n_last_prom}}" >
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Title</th>
			<th >W/s /FDP/STTP</th>
			<th colspan="2">Duration</th>
			<th>Total no. of Days</th>
			<th colspan="2">Role</th>
			<th>Venue</th>
		</tr>
	</thead>
	<tbody id="annexure_n">
		<tr style="background-color:#e3e3e3;">
			<th colspan="3"></th>
			<th>From</th>
			<th>To</th>
			<th colspan="1"></th>
			<th>As Conventor/Coordinator</th>
			<th>Other(Specify)</th>
			<th colspan="2"></th>
		</tr>
		{% for item in annex_n %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.ws}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.tot_day}}</td>
			<td>{{item.roles}}</td>
			<td>{{item.other}}</td>
			<td>{{item.venue}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Conducting national programmes like GIAN etc. (Annexure O)</h4></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_o_last_prom" value="{{annex_o_last_prom}}" required>
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Title</th>
			<th >GIAN/Other(Specify)</th>
			<th colspan="2">Duration</th>
			<th>Total no. of Weeks</th>
			<th colspan="2">Role</th>
			<th>Venue</th>
		</tr>
	</thead>
	<tbody id="annexure_o">
		<tr style="background-color:#e3e3e3;">
			<th colspan="3"></th>
			<th>From</th>
			<th>To</th>
			<th colspan="1"></th>
			<th>As Conventor/Coordinator</th>
			<th>Other(Specify)</th>
			<th colspan="2"></th>
		</tr>
		{% for item in annex_o %}
