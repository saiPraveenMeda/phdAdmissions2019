			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.gian}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.tot_weeks}}</td>
			<td>{{item.roles}}</td>
			<td>{{item.other}}</td>
			<td>{{item.venue}}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>National /International Conference organized as Chairman/Secretary (Annexure P)</h4></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_p_last_prom" value="{{annex_p_last_prom}}" required>
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th >Title of the conference</th>
			<th >Level</th>
			<th colspan="2">Duration</th>
			<th colspan="2">Role</th>
			<th>Venue</th>
		</tr>
	</thead>
	<tbody id="annexure_p">
		<tr style="background-color:#e3e3e3;">
			<th colspan="2"></th>
			<th>National/International</th>
			<th>From</th>
			<th>To</th>
			<th colspan="1"></th>
			<th>As Chairman/Secretary</th>
			<th>Other(Specify)</th>
			<th></th>
		</tr>
		{% for item in annex_p %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.title}}</td>
			<td>{{item.level}}</td>
			<td>{{item.from}}</td>
			<td>{{item.to_date}}</td>
			<td>{{item.roles}}</td>
			<td>{{item.other}}</td>
			<td>{{item.venue}}</td>
		</tr>
