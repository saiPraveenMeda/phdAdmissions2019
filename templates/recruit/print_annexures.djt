	</tbody>
</table>
<div class="pagebreak"> </div>
<div class="row">
	<center><h4>Length of Service (Annexure Q)</h4></center><br>
</div>
<div class="row">
	<div class="col-sm-4"> <b>Date of Last Promotion :</b> </div>
	<div class="col-sm-7">
		<div class="form-group">
			<input class="form-control" type="date" name="annex_q_last_prom" value="{{annex_q_last_prom}}" required>
		</div>
	</div>
</div>
<table class="table table-bordered">
	<thead>
		<tr style="background-color:#e3e3e3;">
			<th >S.No.</th>
			<th colspan="7">Details of teaching experience since last promotion</th>
		</tr>
	</thead>
	<tbody id="annexure_q">
		<tr style="background-color:#e3e3e3;">
			<th colspan="1"></th>
			<th>AGP</th>
			<th>Place</th>
			<th>From</th>
			<th>To</th>
			<th colspan="2">Total</th>
			<th colspan="2"></th>
		</tr>
		<tr>
			<th colspan="6"></th>
			<th>Yrs</th>
			<th>Mnth</th>
		</tr>	
		{% for item in annex_q %}
		<tr>
			<td>{{forloop.counter}}</td>
			<td>{{item.post}}</td>
			<td>{{item.agp}}</td>
